.seat {
	float:left;
    width: 30%;
    height: 100px;
    margin-left:2%;
    background-image:url(http://brandonwei-10027562.cos.myqcloud.com/background.png);
    background-position:center;
    border-radius : 5px;
}
.head {
	width:30%;
	height:20%
}
.row {
	float:left;
	width: 100%;
	margin-top:20px;
}
.audio {
    width: 0;
    height: 0;
    display: none
}
.number {
    width: 20%;
    margin-left:13%;
    float:left;
    height: 40px;
    color: #59b84b;
    font-size: 40px;
}
.job_icon {
    position:absolute;
    z-index:999;
    margin-left:2%;
    float:left;
    width: 8%;
    height: 30px;
    border-radius : 5px;
}
.row_icon {
    float:left;
    width: 100%;
}

.message {
    width: 100%;
    height: 150px;
}
.time {
    display: block;    
    text-align: center;
    height: 75px;
    font-size: 40px;
}
.choose {
    display: block;
    text-align: center;
    height: 75px;
    font-size: 40px;
}